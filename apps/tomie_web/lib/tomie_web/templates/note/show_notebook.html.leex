<nav>
  <ul>
    <li><%= live_patch "Back", to: Routes.live_path(@socket, NoteLive.Index) %></li>
    <li><%= live_patch "Edit", to: Routes.live_path(@socket, NoteLive.EditNotebook, @notebook) %></li>
    <li><%= live_patch "New Note", to: Routes.live_path(@socket, NoteLive.NewNote, @notebook, parent: @root) %></li>
  </ul>
</nav>

<h1> <%= @notebook.title %></h1>

<%= for node <- @nodes do %>
  <%= live_component @socket, TomieWeb.NoteLive.NoteComponent, note: node %>
<% end %>
